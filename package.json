{
  "name": "bmad-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "docker-compose up -d && npm run dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:reset": "tsx scripts/safe-reset-database.ts",
    "seed:complete": "npx tsx scripts/seed-production-ready.ts",
    "seed:dev": "npx tsx scripts/seed-dev-complete.ts",
    "seed:test": "npx tsx scripts/test-seed.ts",
    "build": "prisma generate && next build",
    "vercel-build": "node scripts/safe-migrate.js",
    "vercel-build-no-migrate": "prisma generate && next build",
    "prisma:migrate:prod": "prisma migrate deploy",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "postinstall": "prisma generate",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:debug": "./scripts/debug-tools.sh",
    "dev:health": "./scripts/debug-tools.sh health",
    "dev:clean": "./scripts/debug-tools.sh clean",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:restore": "tsx scripts/restore-database.ts",
    "db:reset-unsafe": "prisma migrate reset",
    "db:migrate-postgres": "tsx scripts/migrate-to-postgres.ts",
    "db:setup-rls": "psql $DATABASE_URL -f prisma/migrations/rls-policies.sql",
    "setup:production": "npm run db:migrate-postgres && npm run db:setup-rls && npm run db:generate",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "clean:console": "tsx scripts/remove-console-logs.ts",
    "build:production": "npm run clean:console && npm run build",
    "env:validate": "tsx lib/config/env-validation.ts",
    "security:audit": "tsx scripts/security-audit.ts",
    "security:check": "npm run security:audit && npm audit",
    "pre-deploy": "npm run env:validate && npm run security:audit && npm run type-check"
  },
  "dependencies": {
    "@axiomhq/js": "^1.3.1",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@node-rs/argon2": "^2.0.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.5.0",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/qrcode": "^1.5.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.13",
    "html2canvas": "^1.4.1",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "lucia": "^3.2.2",
    "lucide-react": "^0.539.0",
    "next": "^15.5.2",
    "qrcode": "^1.5.4",
    "qrcode.js": "^0.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-qr-code": "^2.0.18",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "twilio": "^5.9.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@inquirer/prompts": "^7.8.4",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@playwright/test": "^1.55.0",
    "@prisma/client": "^5.22.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "happy-dom": "^18.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.2.1",
    "playwright": "^1.55.0",
    "prettier": "^3.2.5",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
